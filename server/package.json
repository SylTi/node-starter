{
  "name": "starterkit",
  "version": "1.0.0",
  "description": "StarterKit: Docker Node Functionnal Typescript",
  "main": "dist/server.js",
  "scripts": {
    "pretest": "npm run lint",
    "test": "mocha --no-timeouts --exit -r ts-node/register tests/**/*.ts",
    "prestart": "npm run build",
    "start": "node dist/server.js",
    "start:dev": "ts-node src/server.ts",
    "prebuild": "npm run lint",
    "build": "tsc",
    "lint": "tslint -c tslint.json -p tsconfig.json 'src/**/*.ts' --fix",
    "docker:build:prod": "docker build starterkit-prod:0.0.1 --target production ."
  },
  "keywords": [],
  "author": "SylTi <sylti@unblockchain.tech>",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.5",
    "@types/express": "^4.17.2",
    "@types/node": "^12.12.6",
    "@types/ramda": "^0.26.33",
    "ts-node": "^8.4.1"
  },
  "dependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "list": "^2.0.19",
    "mocha": "^6.2.2",
    "ramda": "^0.26.1",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  }
}
