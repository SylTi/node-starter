{
  "name": "starterkit",
  "version": "1.0.0",
  "description": "StarterKit: Docker Node Functionnal Typescript",
  "main": "dist/server.js",
  "scripts": {
    "pretest": "npm run lint",
    "test": "mocha --no-timeouts --exit -r ts-node/register tests/**/*.ts",
    "prestart": "npm run build",
    "start": "node dist/server.js",
    "start:dev": "ts-node src/server.ts",
    "prebuild": "npm run lint",
    "build": "tsc",
    "lint": "tslint -c tslint.json -p tsconfig.json 'src/**/*.ts' --fix",
    "docker:build:prod": "docker build starterkit-prod:0.0.1 --target production ."
  },
  "keywords": [],
  "author": "SylTi <sylti@unblockchain.tech>",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.28",
    "@types/ramda": "^0.27.38",
    "ts-node": "^9.1.1"
  },
  "dependencies": {
    "chai": "^4.3.0",
    "chai-as-promised": "^7.1.1",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "list": "^2.0.19",
    "mocha": "^8.3.0",
    "ramda": "^0.27.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.5"
  }
}
